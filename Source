	Name = "Rainbow Line",
	Default = false,
	Callback = function(Value)
		if Value == true then
			RainbowLine_Function = game:GetService("RunService").RenderStepped:Connect(function()
				game.ReplicatedStorage.DataEvents.UpdateLineColorsEvent:FireServer(ColorSequence.new(Color3.new(math.random(0,1), math.random(0,1), math.random(0,1))),Color3.fromRGB(255, 0, 0),Color3.fromRGB(255, 0, 0),Color3.fromRGB(255, 0, 0),Color3.fromRGB(255, 0, 0),Color3.fromRGB(255, 0, 0),Color3.fromRGB(255, 0, 0),Color3.fromRGB(255, 0, 0),Color3.fromRGB(255, 0, 0),Color3.new(math.random(0,1),math.random(0,1),math.random(0,1)))
			end)
		else
			RainbowLine_Function:Disconnect()
		end
	end    
})

BlobmanTab:AddSection({Name = "Single Target"})

local BlobmanTarget_DropDown = BlobmanTab:AddDropdown({
	Name = "Target",
	Default = "...",
	Options = GetPlayerList(false,true),
	Callback = function(Value)
		if GetPlayerFromDisplayName(Value) == nil then
			return
		end
		BlobmanTarget_Value = GetPlayerFromDisplayName(Value).Name
	end    
})
BlobmanTab:AddButton({
	Name = "Grab",
	Callback = function()
		if game.Players:FindFirstChild(BlobmanTarget_Value) then
			if GetBlob() ~= nil then
				BlobGrab(GetBlob(),game.Players:FindFirstChild(BlobmanTarget_Value).Character.HumanoidRootPart,"Left")
			end
		end
	end    
})
BlobmanTab:AddButton({
	Name = "Kick",
	Callback = function()
		if game.Players:FindFirstChild(BlobmanTarget_Value) then
			if GetBlob() ~= nil then
				local Character = game.Players:FindFirstChild(BlobmanTarget_Value).Character
				BlobGrab(GetBlob(),Character.HumanoidRootPart,"Left")
				wait(0.1)
				BlobDrop(GetBlob(),Character.HumanoidRootPart,"Left")
				wait(0.1)
				BlobGrab(GetBlob(),Character.HumanoidRootPart,"Left")
				wait(0.1)
				BlobDrop(GetBlob(),Character.HumanoidRootPart,"Left")
				wait(0.1)
				SetNetworkOwner:FireServer(Character.Torso,Character.Torso.CFrame)
				wait(0.05)
				SetNetworkOwner:FireServer(Character.Torso,Character.Torso.CFrame)
				wait(0.1)
				local BF = Instance.new("BodyForce")
				BF.Force = Vector3.new(0,50,0)
				BF.Force = Vector3.new(9999,9999,9999)
				BF.Parent = Character.HumanoidRootPart
				wait(0.5)
				DestroyGrabline:FireServer(Character.Torso)
				wait(0.05)
				BlobGrab(GetBlob(),Character.HumanoidRootPart,"Left")
				BF:Destroy()
			end
		end
	end    
})
BlobmanTab:AddButton({
	Name = "Silent Kick",
	Callback = function()
		if game.Players:FindFirstChild(BlobmanTarget_Value) then
			if GetBlob() ~= nil and GetBlob():IsDescendantOf(SpawnedInToys) == true then
				local Character = game.Players:FindFirstChild(BlobmanTarget_Value).Character
				local Original = plr.Character.HumanoidRootPart.CFrame
				GetBlob().PrimaryPart.CFrame = CFrame.new(Vector3.new(0,80,0)+Character.HumanoidRootPart.Position)
				wait(0.05)
				GetBlob().PrimaryPart.CFrame = CFrame.new(Vector3.new(0,80,0)+Character.HumanoidRootPart.Position)
				wait(0.05)
				BlobGrab(GetBlob(),Character.HumanoidRootPart,"Left")
				wait(0.1)
				BlobDrop(GetBlob(),Character.HumanoidRootPart,"Left")
				wait(0.1)
				BlobGrab(GetBlob(),Character.HumanoidRootPart,"Left")
				wait(0.05)
				DestroyToy:FireServer(GetBlob())
				wait(0.1)
				plr.Character.HumanoidRootPart.CFrame = Original
				plr.Character.HumanoidRootPart.AssemblyLinearVelocity = Vector3.zero
				wait(0.01)
				plr.Character.HumanoidRootPart.CFrame = Original
			end
		end
	end    
})
BlobmanTab:AddButton({
	Name = "Bring",
	Callback = function()
		if game.Players:FindFirstChild(BlobmanTarget_Value) then
			if GetBlob() ~= nil then
				local Character = game.Players:FindFirstChild(BlobmanTarget_Value).Character
				BlobGrab(GetBlob(),Character.HumanoidRootPart,"Left")
				wait(0.1)
				BlobDrop(GetBlob(),Character.HumanoidRootPart,"Left")
				wait(0.1)
				BlobGrab(GetBlob(),Character.HumanoidRootPart,"Left")
				wait(0.1)
				BlobDrop(GetBlob(),Character.HumanoidRootPart,"Left")
			end
		end
	end    
})

