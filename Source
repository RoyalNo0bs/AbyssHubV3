local LoopTarget_DropDown = LoopTab:AddDropdown({
	Name = "Target",
	Default = "...",
	Options = GetPlayerList(false,true),
	Callback = function(Value)
		if GetPlayerFromDisplayName(Value) == nil then
			return
		end
		LoopTarget_Value = GetPlayerFromDisplayName(Value).Name
	end    
})
LoopTab:AddToggle({
	Name = "Loop Kill",
	Default = false,
	Callback = function(Value)
		if Value == true then
			if game.Players:FindFirstChild(LoopTarget_Value) == nil then
				return
			end
			LoopKill_Function = game.Players:FindFirstChild(LoopTarget_Value).CharacterAdded:Connect(function()
				wait(0.05)
				if game.Players:FindFirstChild(LoopTarget_Value).Character and game.Players:FindFirstChild(LoopTarget_Value).Character:FindFirstChild("HumanoidRootPart") then
					local Character = game.Players:FindFirstChild(LoopTarget_Value).Character
					local Original = plr.Character.HumanoidRootPart.CFrame
					plr.Character.HumanoidRootPart.CFrame = Character.HumanoidRootPart.CFrame
					plr.Character.HumanoidRootPart.AssemblyLinearVelocity = Vector3.zero
					wait(0.05)
					SetNetworkOwner:FireServer(Character.Torso,Character.Torso.CFrame)
					wait(0.05)
					plr.Character.HumanoidRootPart.CFrame = Character.HumanoidRootPart.CFrame
					plr.Character.HumanoidRootPart.AssemblyLinearVelocity = Vector3.zero
					wait(0.05)
					SetNetworkOwner:FireServer(Character.Torso,Character.Torso.CFrame)
					wait(0.05)
					plr.Character.HumanoidRootPart.CFrame = Character.HumanoidRootPart.CFrame
					plr.Character.HumanoidRootPart.AssemblyLinearVelocity = Vector3.zero
					wait(0.05)
					SetNetworkOwner:FireServer(Character.Torso,Character.Torso.CFrame)
					wait(0.05)
					Character.HumanoidRootPart.AssemblyLinearVelocity = Vector3.new(99999,0,0)
					wait(0.1)
					Character.HumanoidRootPart.AssemblyLinearVelocity = Vector3.new(99999,0,0)
					wait(0.05)
					DestroyGrabline:FireServer(Character.Torso)
					wait(0.05)
					Character.HumanoidRootPart.AssemblyLinearVelocity = Vector3.new(0,-9999,0)
					plr.Character.HumanoidRootPart.CFrame = Original
					plr.Character.HumanoidRootPart.AssemblyLinearVelocity = Vector3.zero
					wait(0.05)
					plr.Character.HumanoidRootPart.CFrame = Original
					plr.Character.HumanoidRootPart.AssemblyLinearVelocity = Vector3.zero
				end
			end)
			if game.Players:FindFirstChild(LoopTarget_Value).Character and game.Players:FindFirstChild(LoopTarget_Value).Character:FindFirstChild("HumanoidRootPart") then
				local Character = game.Players:FindFirstChild(LoopTarget_Value).Character
				local Original = plr.Character.HumanoidRootPart.CFrame
				plr.Character.HumanoidRootPart.CFrame = Character.HumanoidRootPart.CFrame
				plr.Character.HumanoidRootPart.AssemblyLinearVelocity = Vector3.zero
				wait(0.05)
				SetNetworkOwner:FireServer(Character.Torso,Character.Torso.CFrame)
				wait(0.05)
				plr.Character.HumanoidRootPart.CFrame = Character.HumanoidRootPart.CFrame
				plr.Character.HumanoidRootPart.AssemblyLinearVelocity = Vector3.zero
				wait(0.05)
				SetNetworkOwner:FireServer(Character.Torso,Character.Torso.CFrame)
				wait(0.05)
				plr.Character.HumanoidRootPart.CFrame = Character.HumanoidRootPart.CFrame
				plr.Character.HumanoidRootPart.AssemblyLinearVelocity = Vector3.zero
				wait(0.05)
				SetNetworkOwner:FireServer(Character.Torso,Character.Torso.CFrame)
				wait(0.05)
				Character.HumanoidRootPart.AssemblyLinearVelocity = Vector3.new(99999,0,0)
				wait(0.1)
				Character.HumanoidRootPart.AssemblyLinearVelocity = Vector3.new(99999,0,0)
				wait(0.05)
				DestroyGrabline:FireServer(Character.Torso)
				wait(0.05)
				Character.HumanoidRootPart.AssemblyLinearVelocity = Vector3.new(0,-9999,0)
				plr.Character.HumanoidRootPart.CFrame = Original
				plr.Character.HumanoidRootPart.AssemblyLinearVelocity = Vector3.zero
				wait(0.05)
				plr.Character.HumanoidRootPart.CFrame = Original
				plr.Character.HumanoidRootPart.AssemblyLinearVelocity = Vector3.zero
			end
		else
			LoopKill_Function:Disconnect()
		end
	end    
})

local RemainingTime = AutoTab:AddParagraph("Remaining Time: "..workspace.Slots:FindFirstChild("Slots").Screen.SlotGui.TimeLeftFrame.TimeText.Text,"")
AutoTab:AddToggle({
	Name = "Auto Farm",
	Default = false,
	Callback = function(Value)
		if Value == true then
			AutoFarm_Function = game:GetService("RunService").RenderStepped:Connect(function()
				local Original = plr.Character.HumanoidRootPart.CFrame
				if workspace.Slots:FindFirstChild("Slots").SlotHandle.LightBall.Material == Enum.Material.Neon then
					for _, LightBall in ipairs(workspace.Slots:GetDescendants()) do
						if LightBall:IsA("MeshPart") and LightBall.Name == "LightBall" then
							plr.Character.HumanoidRootPart.CFrame = CFrame.new(LightBall.Position + Vector3.new(0,5,0))
							plr.Character.HumanoidRootPart.AssemblyLinearVelocity = Vector3.zero
							wait(0.05)
							SetNetworkOwner:FireServer(LightBall.Parent.Handle,LightBall.CFrame)
							wait(0.05)
							plr.Character.HumanoidRootPart.CFrame = CFrame.new(LightBall.Position + Vector3.new(0,5,0))
							plr.Character.HumanoidRootPart.AssemblyLinearVelocity = Vector3.zero
							wait(0.05)
							SetNetworkOwner:FireServer(LightBall.Parent.Handle,LightBall.CFrame)
							wait(0.05)
							plr.Character.HumanoidRootPart.CFrame = CFrame.new(LightBall.Position + Vector3.new(0,5,0))
							plr.Character.HumanoidRootPart.AssemblyLinearVelocity = Vector3.zero
							wait(0.05)
							SetNetworkOwner:FireServer(LightBall.Parent.Handle,LightBall.CFrame)
							wait(0.1)
							plr.Character.HumanoidRootPart.CFrame = CFrame
							plr.Character.HumanoidRootPart.AssemblyLinearVelocity = Vector3.zero
						end
					end
				end
			end)
		else
			AutoFarm_Function:Disconnect()
		end
	end    
})

ConfigurationTab:AddSection({Name = "Toy Selection"})

ConfigurationTab:AddDropdown({
	Name = "Fire Toy",
	Default = "...",
	Options = {"Campfire","OvenMicrowaveWhite","BathroomSink","CounterSink","OvenRusty","OvenDarkGray"},
	Callback = function(Value)
		FirePartToy = Value
	end    
})
ConfigurationTab:AddDropdown({
	Name = "Radiation Toy",
	Default = "...",
	Options = {"OvenMicrowaveWhite","UFO"},
	Callback = function(Value)
		RadiationPartToy = Value
	end    
})

CreditsTab:AddParagraph("@Royal_No0bs (Royal Noob)","Scripted Everything")
CreditsTab:AddParagraph("@Arodriguezus4 (Blocky)","Generated Ideas")

workspace.Slots:FindFirstChild("Slots").Screen.SlotGui.TimeLeftFrame.TimeText.Changed:Connect(function()
	RemainingTime:Set("Remaining Time: "..workspace.Slots:FindFirstChild("Slots").Screen.SlotGui.TimeLeftFrame.TimeText.Text,"")
end)

workspace.ChildAdded:Connect(function(GrabParts)
	if GrabParts:IsA("Model") and GrabParts.Name == "GrabParts" then
		local Part = GrabParts:WaitForChild("GrabPart"):WaitForChild("WeldConstraint").Part1
		task.spawn(function()
			if PoisonGrab_Toggle == true then
				if Part.Parent:FindFirstChild("Humanoid") then
					local PoisonPart = workspace.Map.Hole.PoisonBigHole.PoisonHurtPart
					while GrabParts:IsDescendantOf(workspace) do
						PoisonPart.Size = Vector3.new(1,1,1)
						PoisonPart.Position = Part.Parent:FindFirstChild("Head").Position
						wait()
					end
					PoisonPart.Size = Vector3.new(16.5, 7.625, 17.5)
					PoisonPart.Position = Vector3.new(124.325, 347.506, 309.705)
				end
			end
		end)
		task.spawn(function()
			if RadiationGrab_Toggle == true then
				if Part.Parent:FindFirstChild("Humanoid") then
					local RadiationPart = workspace.Map.AlwaysHereTweenedObjects.InnerUFO.Object.ObjectModel:FindFirstChild("PaintPlayerPart")
					while GrabParts:IsDescendantOf(workspace) do
						RadiationPart.Size = Vector3.new(1,1,1)
						RadiationPart.Position = Part.Parent:FindFirstChild("Head").Position
						wait()
					end
					RadiationPart.Size = Vector3.new(0,0,0)
					RadiationPart.Position = Vector3.new(0,0,0)
				end
			end
		end)
		task.spawn(function()
			if FireGrab_Toggle == true then
				while GrabParts:IsDescendantOf(workspace) do
					local chr = plr.Character
					local HRP = chr:WaitForChild("HumanoidRootPart")
					if SpawnedInToys:FindFirstChild("Campfire") then
						local Campfire = SpawnedInToys:FindFirstChild("Campfire")
						if Campfire.PrimaryPart:FindFirstChild("BodyPosition") == nil then
							local BP = Instance.new("BodyPosition")
							BP.MaxForce = Vector3.new(math.huge,math.huge,math.huge)
							BP.Position = Vector3.new(0,250,0)
							BP.Parent = Campfire.PrimaryPart
							BP.P = 99999
						end
						if Campfire:FindFirstChild("SoundPart"):FindFirstChild("PartOwner") == nil then
							local Original = HRP.CFrame
							SetNetworkOwner:FireServer(Campfire:FindFirstChild("SoundPart"),Campfire:FindFirstChild("SoundPart").CFrame)
							HRP.CFrame = CFrame.new(Campfire:FindFirstChild("SoundPart").Position+Vector3.new(0,10,0))
							HRP.AssemblyLinearVelocity = Vector3.zero
							wait(0.05)
							SetNetworkOwner:FireServer(Campfire:FindFirstChild("SoundPart"),Campfire:FindFirstChild("SoundPart").CFrame)
							HRP.CFrame = CFrame.new(Campfire:FindFirstChild("SoundPart").Position+Vector3.new(0,10,0))
							HRP.AssemblyLinearVelocity = Vector3.zero
							wait(0.05)
							SetNetworkOwner:FireServer(Campfire:FindFirstChild("SoundPart"),Campfire:FindFirstChild("SoundPart").CFrame)
							HRP.CFrame = CFrame.new(Campfire:FindFirstChild("SoundPart").Position+Vector3.new(0,10,0))
							HRP.AssemblyLinearVelocity = Vector3.zero
							wait(0.1)
							HRP.AssemblyLinearVelocity = Vector3.zero
							HRP.CFrame = Original
							wait(0.05)
							HRP.AssemblyLinearVelocity = Vector3.zero
							HRP.CFrame = Original
							wait(0.05)
							HRP.AssemblyLinearVelocity = Vector3.zero
							HRP.CFrame = Original
						end
						Campfire:FindFirstChild("FirePlayerPart").Size = Vector3.new(3,3,3)
						Campfire:FindFirstChild("FirePlayerPart").Position = Part.Parent:FindFirstChild("Head").Position
					else
						SpawnToyRemoteFunction:InvokeServer("Campfire",HRP.CFrame,Vector3.new(0,0,0))
					end
					wait()
				end
			end
		end)
		task.spawn(function()
			if MasslessGrab_Toggle == true and Part.Anchored == false then
				while GrabParts:IsDescendantOf(workspace) do
					local chr = plr.Character
					local HRP = chr:WaitForChild("HumanoidRootPart")
					if Part.Parent then
						for _, p in ipairs(Part.Parent:GetChildren()) do
							if p:IsA("BasePart") then
								p.Massless = true
							end
						end
					else
						Part.Massless = true
					end
					wait()
				end
				if Part.Parent then
					for _, p in ipairs(Part.Parent:GetChildren()) do
						if p:IsA("BasePart") then
							p.Massless = false
						end
					end
				else
					Part.Massless = false
				end
			end
		end)
		task.spawn(function()
			if NoclipGrab_Toggle == true and Part.Anchored == false then
				while GrabParts:IsDescendantOf(workspace) do
					local chr = plr.Character
					local HRP = chr:WaitForChild("HumanoidRootPart")
					if Part.Parent then
						for _, p in ipairs(Part.Parent:GetDescendants()) do
							if p:IsA("BasePart") then
								p.CanCollide = false
							end
						end
					else
						Part.CanCollide = false
					end
					wait()
				end
				if Part.Parent then
					for _, p in ipairs(Part.Parent:GetDescendants()) do
						if p:IsA("BasePart") and p.Transparency ~= 1 then
							p.CanCollide = true
						end
					end
				else
					Part.CanCollide = true
				end
			end
		end)
		task.spawn(function()
			if DestroyGrab_Toggle == true and Part.Anchored == false then
				while GrabParts:IsDescendantOf(workspace) do
					local chr = plr.Character
					local HRP = chr:WaitForChild("HumanoidRootPart")
					for _, p in ipairs(Part.Parent:GetDescendants()) do
						if p:IsA("BasePart") then
							p.CanCollide = false
						end
					end
					Part.AssemblyLinearVelocity = Vector3.new(0,999999,0)
					wait()
				end
				for _, p in ipairs(Part.Parent:GetDescendants()) do
					if p:IsA("BasePart") and p.Transparency ~= 1 then
						p.CanCollide = true
					end
				end
				Part.AssemblyLinearVelocity = Vector3.zero
			end
		end)
		task.spawn(function()
			if KillGrab_Toggle == true and Part.Anchored == false and Part.Parent:FindFirstChild("Humanoid") then
				wait(0.2)
				Part.AssemblyLinearVelocity = Vector3.new(99999,0,0)
				wait(0.1)
				DestroyGrabline:FireServer(Part)
				wait(0.05)
				Part.AssemblyLinearVelocity = Vector3.new(0,-9999,0)
			end
		end)
		local i = false
		wait(0.02)
		UIS.InputBegan:Connect(function(Input)
			if Input.UserInputType == Enum.UserInputType.MouseButton2 and ThrowPower_Toggle == true and i == false then
				ThrowPart(Part)
				i = true
			end
			if Input.UserInputType == Enum.UserInputType.MouseButton1 and i == false then
				wait(0.05)
				i = true
			end
		end)
	end
end)

game.Players.PlayerAdded:Connect(function(plr)
	BlobmanTarget_DropDown:Refresh(GetPlayerList(false,true),true)
	LoopTarget_DropDown:Refresh(GetPlayerList(false,true),true)
	ImmunityTarget_DropDown:Refresh(GetPlayerList(false,true),true)
end)
game.Players.PlayerRemoving:Connect(function(plr)
	BlobmanTarget_DropDown:Refresh(GetPlayerList(false,true),true)
	LoopTarget_DropDown:Refresh(GetPlayerList(false,true),true)
	ImmunityTarget_DropDown:Refresh(GetPlayerList(false,true),true)
end)
plr.Character.Humanoid.Changed:Connect(function()
	if WalkSpeed_Toggle == true then
		plr.Character.Humanoid.WalkSpeed = WalkSpeed_Value
	end
end)

plr.CharacterAdded:Connect(function(chr)
	chr:WaitForChild("Humanoid").Changed:Connect(function()
		if WalkSpeed_Toggle == true then
			chr.Humanoid.WalkSpeed = WalkSpeed_Value
		end
	end)
end)

local Held = false
UIS.InputBegan:Connect(function(input,gp)
	if gp then
		return
	end
	if input.KeyCode == Enum.KeyCode.Space and InfiniteJump_Toggle == true and plr.Character then
		Held = true
		while Held == true do
			plr.Character.Humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
			wait(0.01)
		end
	end
end)

UIS.InputEnded:Connect(function(input)
	if input.KeyCode == Enum.KeyCode.Space then
		Held = false
	end
end)


OrionLib:Init()


