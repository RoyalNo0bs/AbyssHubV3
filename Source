--FUNCTIONS--
function GetPlayerList(Bool,Bool2)
	local List = {}
	for _, Player in ipairs(game.Players:GetPlayers()) do
		if Player == plr and Bool == true then
			if Bool2 == true then
				table.insert(List,Player.Name.."("..Player.DisplayName..")")
			else
				table.insert(List,Player.Name)
			end
		elseif Player ~= plr then
			if Bool2 == true then
				table.insert(List,Player.Name.."("..Player.DisplayName..")")
			else
				table.insert(List,Player.Name)
			end
		end
	end
	return List
end
function GetNetworkOwner(Toy)
	for _, PartOwner in ipairs(Toy:GetDescendants()) do
		if PartOwner:IsA("StringValue") and PartOwner.Name == "PartOwner" then
			return PartOwner.Value
		end
	end
	return nil
end
function GetToysSpawned(ToyName)
	local n = 0
	for _, Toy in ipairs(SpawnedInToys:GetChildren()) do
		if Toy:IsA("Model") and Toy.Name == ToyName then
			n += 1
		end
	end
	return n
end
function GetPlayerFromDisplayName(Text)
	for _, Player in ipairs(game.Players:GetPlayers()) do
		if string.find(Text,Player.Name) then
			return Player
		end
	end
	return nil
end
function GetBlob()
	local chr = plr.Character
	local hrp = chr:WaitForChild("HumanoidRootPart")
	local hum = chr:WaitForChild("Humanoid")
	for _, blobs in workspace:GetDescendants() do
		if blobs.Name ~= "CreatureBlobman" then continue end
		if not blobs:FindFirstChild("VehicleSeat") then continue end
		if not blobs.VehicleSeat:FindFirstChild("SeatWeld") then continue end
		if blobs.VehicleSeat.SeatWeld.Part1 == hrp then
			return blobs
		end
	end
end
function BlobGrab(Blob,Target,Side)
	local chr = plr.Character
	local hrp = chr:WaitForChild("HumanoidRootPart")
	local hum = chr:WaitForChild("Humanoid")
	local args = {
		[1] = Blob:FindFirstChild(Side.."Detector"),
		[2] = Target,
		[3] = Blob:FindFirstChild(Side.."Detector"):FindFirstChild(Side.."Weld"),
	}
	Blob.BlobmanSeatAndOwnerScript.CreatureGrab:FireServer(unpack(args))
end
function BlobDrop(Blob,Target,Side)
	local chr = plr.Character
	local hrp = chr:WaitForChild("HumanoidRootPart")
	local hum = chr:WaitForChild("Humanoid")
	local args = {
		[1] = Blob:FindFirstChild(Side.."Detector"):FindFirstChild(Side.."Weld"),
		[2] = Target,
	}
	Blob.BlobmanSeatAndOwnerScript.CreatureDrop:FireServer(unpack(args))
end
function KillAll()
	while KillAll_Toggle == true do
		for _, Player in ipairs(game.Players:GetPlayers()) do
			if KillAll_Toggle == true and Player.Character and Player.Character:FindFirstChild("HumanoidRootPart") and Player.Character:FindFirstChild("HumanoidRootPart").AssemblyLinearVelocity.Magnitude < 1000 and Player ~= plr and Player.InPlot.Value == false then
				local TargetHRP = Player.Character:FindFirstChild("HumanoidRootPart")
				local HRP = plr.Character
				local Character = Player.Character
				local Original = plr.Character.HumanoidRootPart.CFrame
				plr.Character.HumanoidRootPart.CFrame = Character.HumanoidRootPart.CFrame
				plr.Character.HumanoidRootPart.AssemblyLinearVelocity = Vector3.zero
				wait(0.05)
				SetNetworkOwner:FireServer(Character.Torso,Character.Torso.CFrame)
				wait(0.05)
				plr.Character.HumanoidRootPart.CFrame = Character.HumanoidRootPart.CFrame
				plr.Character.HumanoidRootPart.AssemblyLinearVelocity = Vector3.zero
				wait(0.05)
				SetNetworkOwner:FireServer(Character.Torso,Character.Torso.CFrame)
				wait(0.05)
				plr.Character.HumanoidRootPart.CFrame = Character.HumanoidRootPart.CFrame
				plr.Character.HumanoidRootPart.AssemblyLinearVelocity = Vector3.zero
				wait(0.05)
				SetNetworkOwner:FireServer(Character.Torso,Character.Torso.CFrame)
				wait(0.05)
				Character.HumanoidRootPart.AssemblyLinearVelocity = Vector3.new(99999,0,0)
				wait(0.1)
				Character.HumanoidRootPart.AssemblyLinearVelocity = Vector3.new(99999,0,0)
				wait(0.05)
				DestroyGrabline:FireServer(Character.Torso)
				wait(0.05)
				Character.HumanoidRootPart.AssemblyLinearVelocity = Vector3.new(0,-9999,0)
				plr.Character.HumanoidRootPart.CFrame = Original
				plr.Character.HumanoidRootPart.AssemblyLinearVelocity = Vector3.zero
				wait(0.05)
				plr.Character.HumanoidRootPart.CFrame = Original
				plr.Character.HumanoidRootPart.AssemblyLinearVelocity = Vector3.zero
			end
		end
		wait(0.05)
	end
end
function RadiationAll()
	while RadiationAll_Toggle == true do
		for _, Player in ipairs(game.Players:GetPlayers()) do
			if RadiationAll_Toggle == true and Player.Character and Player.Character:FindFirstChild("HumanoidRootPart") and Player ~= plr then
				if RadiationPartToy == "UFO" and Player.InPlot.Value == false and Player.Character:FindFirstChild("HumanoidRootPart"):FindFirstChild("PaintPointLight") == nil then
					local TargetHRP = Player.Character:FindFirstChild("HumanoidRootPart")
					local HRP = plr.Character
					local Character = Player.Character
					local Original = plr.Character.HumanoidRootPart.CFrame
					local Lazer  = workspace.Map.AlwaysHereTweenedObjects.OuterUFO.Object.ObjectModel:FindFirstChild("PaintPlayerPart")
					plr.Character.HumanoidRootPart.CFrame = Character.HumanoidRootPart.CFrame
					plr.Character.HumanoidRootPart.AssemblyLinearVelocity = Vector3.zero
					Lazer.Size = Vector3.new(3,3,3)
					Lazer.Position = Character.HumanoidRootPart.Position
					wait(0.05)
					SetNetworkOwner:FireServer(Character.Torso,Character.Torso.CFrame)
					wait(0.05)
					plr.Character.HumanoidRootPart.CFrame = Character.HumanoidRootPart.CFrame
					plr.Character.HumanoidRootPart.AssemblyLinearVelocity = Vector3.zero
					Lazer.Size = Vector3.new(3,3,3)
					Lazer.Position = Character.HumanoidRootPart.Position
					wait(0.05)
					SetNetworkOwner:FireServer(Character.Torso,Character.Torso.CFrame)
					Lazer.Size = Vector3.new(3,3,3)
					Lazer.Position = Character.HumanoidRootPart.Position
					wait(0.05)
					plr.Character.HumanoidRootPart.CFrame = Original
					plr.Character.HumanoidRootPart.AssemblyLinearVelocity = Vector3.zero
					Lazer.Position = Vector3.new(0,1000,0)
					DestroyGrabline:FireServer(Character.Torso)
				elseif RadiationPartToy == "OvenMicrowaveWhite" then
					if SpawnedInToys:FindFirstChild("OvenMicrowaveWhite") == nil then
						SpawnToyRemoteFunction:InvokeServer("OvenMicrowaveWhite",CFrame.new(Vector3.new(0,0,0)),Vector3.new(0,0,0))
						wait(0.1)
					elseif SpawnedInToys:FindFirstChild("OvenMicrowaveWhite") and GetNetworkOwner(SpawnedInToys:FindFirstChild("OvenMicrowaveWhite")) ~= plr.Name then
						local Original = plr.Character.HumanoidRootPart.CFrame
						plr.Character.HumanoidRootPart.CFrame = CFrame.new(SpawnedInToys:FindFirstChild("OvenMicrowaveWhite").PrimaryPart.Position + Vector3.new(0,10,0))
						plr.Character.HumanoidRootPart.AssemblyLinearVelocity = Vector3.zero
						wait(0.05)
						SetNetworkOwner:FireServer(SpawnedInToys:FindFirstChild("OvenMicrowaveWhite"):FindFirstChild("SoundPart"),SpawnedInToys:FindFirstChild("OvenMicrowaveWhite"):FindFirstChild("SoundPart").CFrame)
						wait(0.05)
						plr.Character.HumanoidRootPart.CFrame = CFrame.new(SpawnedInToys:FindFirstChild("OvenMicrowaveWhite").PrimaryPart.Position + Vector3.new(0,10,0))
						plr.Character.HumanoidRootPart.AssemblyLinearVelocity = Vector3.zero
						wait(0.05)
						SetNetworkOwner:FireServer(SpawnedInToys:FindFirstChild("OvenMicrowaveWhite"):FindFirstChild("SoundPart"),SpawnedInToys:FindFirstChild("OvenMicrowaveWhite"):FindFirstChild("SoundPart").CFrame)
						wait(0.05)
						plr.Character.HumanoidRootPart.CFrame = Original
						plr.Character.HumanoidRootPart.AssemblyLinearVelocity = Vector3.zero
						wait()
						plr.Character.HumanoidRootPart.CFrame = Original
						plr.Character.HumanoidRootPart.AssemblyLinearVelocity = Vector3.zero
					elseif SpawnedInToys:FindFirstChild("OvenMicrowaveWhite") and GetNetworkOwner(SpawnedInToys:FindFirstChild("OvenMicrowaveWhite")) == plr.Name and SpawnedInToys:FindFirstChild("OvenMicrowaveWhite").Button.Transparency == 0 then
						while SpawnedInToys:FindFirstChild("OvenMicrowaveWhite").Button.Transparency == 0 or SpawnedInToys:FindFirstChild("OvenMicrowaveWhite") == nil do
							if SpawnedInToys:FindFirstChild("OvenMicrowaveWhite") and SpawnedInToys:FindFirstChild("OvenMicrowaveWhite").Button.Transparency == 0 then
								plr.Character.HumanoidRootPart.CFrame = CFrame.new(SpawnedInToys:FindFirstChild("OvenMicrowaveWhite").PrimaryPart.Position + Vector3.new(0,10,0))
								plr.Character.HumanoidRootPart.AssemblyLinearVelocity = Vector3.zero
								wait(0.05)
								SetNetworkOwner:FireServer(SpawnedInToys:FindFirstChild("OvenMicrowaveWhite").Button,SpawnedInToys:FindFirstChild("OvenMicrowaveWhite").Button.CFrame)
								wait(0.2)
							end
						end
					end
					
					if SpawnedInToys:FindFirstChild("OvenMicrowaveWhite") then
						local OvenMicrowaveWhite = SpawnedInToys:FindFirstChild("OvenMicrowaveWhite")
						if OvenMicrowaveWhite.PrimaryPart:FindFirstChild("BodyPosition") == nil then
							local BP = Instance.new("BodyPosition")
							BP.P = 5000
							BP.D = 100
							BP.MaxForce = Vector3.new(math.huge,math.huge,math.huge)
							BP.Parent = OvenMicrowaveWhite.PrimaryPart
							BP = Vector3.new(0,90,0)
							if OvenMicrowaveWhite.PrimaryPart:FindFirstChild("BodyGyro") == nil then
								local BG = Instance.new("BodyGyro")
								BG.P = 5000
								BG.MaxTorque = Vector3.new(math.huge,math.huge,math.huge)
								BG.D = 100
								BG.CFrame = CFrame.new(0,0,0)
								BG.Parent = OvenMicrowaveWhite.PrimaryPart
							end
						end
						local BP = OvenMicrowaveWhite.PrimaryPart:FindFirstChild("BodyPosition")
						BP.Position = Player.Character.HumanoidRootPart.Position + Vector3.new(0,100,0)
						OvenMicrowaveWhite.PaintPlayerPart.Size = Vector3.new(50,200,50)
						wait(0.1)
					end
				end
			end
		end
		wait(0.05)
	end
end
function FireAll()
	while FireAll_Toggle == true do
		for _, Player in ipairs(game.Players:GetPlayers()) do
			if FireAll_Toggle == true and Player.Character and Player.Character:FindFirstChild("HumanoidRootPart") and Player.Character:FindFirstChild("HumanoidRootPart"):FindFirstChild("PaintPointLight") == nil and Player ~= plr and Player.InPlot.Value == false then
				if RadiationPartToy == "UFO" then
					local TargetHRP = Player.Character:FindFirstChild("HumanoidRootPart")
					local HRP = plr.Character
					local Character = Player.Character
					local Original = plr.Character.HumanoidRootPart.CFrame
					local Lazer  = workspace.Map.AlwaysHereTweenedObjects.OuterUFO.Object.ObjectModel:FindFirstChild("PaintPlayerPart")
					plr.Character.HumanoidRootPart.CFrame = Character.HumanoidRootPart.CFrame
					plr.Character.HumanoidRootPart.AssemblyLinearVelocity = Vector3.zero
					Lazer.Size = Vector3.new(3,3,3)
					Lazer.Position = Character.HumanoidRootPart.Position
					wait(0.05)
					SetNetworkOwner:FireServer(Character.Torso,Character.Torso.CFrame)
					wait(0.05)
					plr.Character.HumanoidRootPart.CFrame = Character.HumanoidRootPart.CFrame
					plr.Character.HumanoidRootPart.AssemblyLinearVelocity = Vector3.zero
					Lazer.Size = Vector3.new(3,3,3)
					Lazer.Position = Character.HumanoidRootPart.Position
					wait(0.05)
					SetNetworkOwner:FireServer(Character.Torso,Character.Torso.CFrame)
					Lazer.Size = Vector3.new(3,3,3)
					Lazer.Position = Character.HumanoidRootPart.Position
					wait(0.05)
					plr.Character.HumanoidRootPart.CFrame = Original
					plr.Character.HumanoidRootPart.AssemblyLinearVelocity = Vector3.zero
					Lazer.Position = Vector3.new(0,1000,0)
					DestroyGrabline:FireServer(Character.Torso)
				elseif RadiationPartToy == "OvenMicrowaveWhite" then
					if SpawnedInToys:FindFirstChild("OvenMicrowaveWhite") == nil then
						SpawnToyRemoteFunction:InvokeServer("OvenMicrowaveWhite",CFrame.new(Vector3.new(0,0,0)),Vector3.new(0,0,0))
					elseif SpawnedInToys:FindFirstChild("OvenMicrowaveWhite") and GetNetworkOwner(SpawnedInToys:FindFirstChild("OvenMicrowaveWhite")) ~= plr.Name then
						local Original = plr.Character.HumanoidRootPart.CFrame
						plr.Character.HumanoidRootPart.CFrame = SpawnedInToys:FindFirstChild("OvenMicrowaveWhite").PrimaryPart.CFrame
						plr.Character.HumanoidRootPart.AssemblyLinearVelocity = Vector3.zero
						wait(0.05)
						SetNetworkOwner:FireServer(SpawnedInToys:FindFirstChild("OvenMicrowaveWhite"):FindFirstChild("SoundPart"),SpawnedInToys:FindFirstChild("OvenMicrowaveWhite"):FindFirstChild("SoundPart").CFrame)
						wait(0.05)
						plr.Character.HumanoidRootPart.CFrame = SpawnedInToys:FindFirstChild("OvenMicrowaveWhite").PrimaryPart.CFrame
						plr.Character.HumanoidRootPart.AssemblyLinearVelocity = Vector3.zero
						wait(0.05)
						SetNetworkOwner:FireServer(SpawnedInToys:FindFirstChild("OvenMicrowaveWhite"):FindFirstChild("SoundPart"),SpawnedInToys:FindFirstChild("OvenMicrowaveWhite"):FindFirstChild("SoundPart").CFrame)
						wait(0.05)
						plr.Character.HumanoidRootPart.CFrame = Original
						plr.Character.HumanoidRootPart.AssemblyLinearVelocity = Vector3.zero
						wait()
						plr.Character.HumanoidRootPart.CFrame = Original
						plr.Character.HumanoidRootPart.AssemblyLinearVelocity = Vector3.zero
					elseif SpawnedInToys:FindFirstChild("OvenMicrowaveWhite") and GetNetworkOwner(SpawnedInToys:FindFirstChild("OvenMicrowaveWhite")) == plr.Name and SpawnedInToys:FindFirstChild("OvenMicrowaveWhite").Button.Transparency == 0 then
						while SpawnedInToys:FindFirstChild("OvenMicrowaveWhite").Button.Transparency == 0 do
							if SpawnedInToys:FindFirstChild("OvenMicrowaveWhite").Button.Transparency == 0 then
								plr.Character.HumanoidRootPart.CFrame = SpawnedInToys:FindFirstChild("OvenMicrowaveWhite").PrimaryPart.CFrame
								plr.Character.HumanoidRootPart.AssemblyLinearVelocity = Vector3.zero
								wait(0.05)
								SetNetworkOwner:FireServer(SpawnedInToys:FindFirstChild("OvenMicrowaveWhite").Button,SpawnedInToys:FindFirstChild("OvenMicrowaveWhite").Button.CFrame)
								wait(0.2)
							end
						end
					end
					local OvenMicrowaveWhite = SpawnedInToys:FindFirstChild("OvenMicrowaveWhite")
					if OvenMicrowaveWhite then
						if OvenMicrowaveWhite.PrimaryPart:FindFirstChild("BodyPosition") == nil then
							local BP = Instance.new("BodyPosition")
							BP.P = 5000
							BP.D = 100
							BP.MaxForce = Vector3.new(math.huge,math.huge,math.huge)
							BP.Parent = OvenMicrowaveWhite.PrimaryPart
							BP = Vector3.new(0,50,0)
							if OvenMicrowaveWhite.PrimaryPart:FindFirstChild("BodyGyro") == nil then
								local BG = Instance.new("BodyGyro")
								BG.P = 5000
								BG.MaxTorque = Vector3.new(math.huge,math.huge,math.huge)
								BG.D = 100
								BG.CFrame = CFrame.new(0,0,0)
								BG.Parent = OvenMicrowaveWhite.PrimaryPart
							end
						end
						local BP = OvenMicrowaveWhite.PrimaryPart:FindFirstChild("BodyPosition")
						for _, Player in ipairs(game.Players:GetPlayers()) do
							if Player.Character and Player.Character:FindFirstChild("HumanoidRootPart") and SpawnedInToys:FindFirstChild("OvenMicrowaveWhite") then
								BP.Position = Player.Character.HumanoidRootPart.Position + Vector3.new(0,50,0)
								wait(0.2)
								OvenMicrowaveWhite.PaintPlayerPart.Size = Vector3.new(50,200,50)
							end
						end
					end
				end
			end
		end
		wait(0.05)
	end
end
function ThrowPart(Part)
	local char = plr.Character
	local hrp = char:WaitForChild("HumanoidRootPart")
	local hum = char:WaitForChild("Humanoid")
	if Part and Part.Parent and ThrowPower_Value then
		local velocityObj = Instance.new("BodyVelocity", Part)
		velocityObj.MaxForce = Vector3.new(math.huge, math.huge, math.huge)
		velocityObj.Velocity = workspace.CurrentCamera.CFrame.lookVector * ThrowPower_Value
		Debris:AddItem(velocityObj, 0.1)
	end
end
function DrawerCannon()
	local KeyBindShoot = nil
	KeyBindShoot = UIS.InputBegan:Connect(function(Input)
		if Input.KeyCode == Enum.KeyCode.Q then
			for _, Drawer in ipairs(SpawnedInToys:GetChildren()) do
				if Drawer:IsA("Model") and Drawer.Name == "DrawerLightBrown" then
					if Drawer.PrimaryPart:FindFirstChild("BodyVelocity") == nil then
						Drawer.Name = "Trashed"
						if Drawer.PrimaryPart:FindFirstChild("BodyPosition") then
							Drawer.PrimaryPart:FindFirstChild("BodyPosition"):Destroy()
						end
						local velocityObj = Instance.new("BodyVelocity", Drawer.PrimaryPart)
						velocityObj.MaxForce = Vector3.new(math.huge, math.huge, math.huge)
						velocityObj.Velocity = (workspace.CurrentCamera.CFrame.lookVector * 1000)-Vector3.new(0,500,0)
						task.spawn(function()
							wait(6)
							DestroyToy:FireServer(Drawer)
						end)
					end

				end
			end
		end
	end)
	while DrawerCannon_Toggle == true do
		local chr = plr.Character
		local hrp = chr:WaitForChild("HumanoidRootPart")
		local hum = chr:WaitForChild("Humanoid")
		while GetToysSpawned("DrawerLightBrown") < DrawerCannon_Value do
			if GetToysSpawned("DrawerLightBrown") < DrawerCannon_Value and DrawerCannonAutoLoad_Toggle == true then
				SpawnToyRemoteFunction:InvokeServer("DrawerLightBrown",CFrame.new(plr.Character.Head.Position+Vector3.new(0,20,0)),Vector3.new(0,0,0))
				wait(0.1)
			end
			wait()
		end
		local OriginalPosition = hrp.CFrame
		for _, Drawer in ipairs(SpawnedInToys:GetChildren()) do
			if Drawer:IsA("Model") and Drawer.Name == "DrawerLightBrown" and Drawer.Part:FindFirstChild("PartOwner") == nil then
				if Drawer.PrimaryPart:FindFirstChild("BodyPosition") then
					Drawer.PrimaryPart:FindFirstChild("BodyPosition"):Destroy()
				end
				hrp.CFrame = CFrame.new(Drawer:GetPivot().Position + Vector3.new(0,0,15))
				hrp.AssemblyLinearVelocity = Vector3.zero
				wait(0.01)
				hrp.CFrame = CFrame.new(Drawer:GetPivot().Position + Vector3.new(0,0,15))
				hrp.AssemblyLinearVelocity = Vector3.zero
				SetNetworkOwner:FireServer(Drawer.Part,Drawer.Part.CFrame)
				wait(0.01)
				hrp.CFrame = CFrame.new(Drawer:GetPivot().Position + Vector3.new(0,0,15))
				hrp.AssemblyLinearVelocity = Vector3.zero
				SetNetworkOwner:FireServer(Drawer.Part,Drawer.Part.CFrame)
				wait(0.01)
				hrp.CFrame = CFrame.new(Drawer:GetPivot().Position + Vector3.new(0,0,15))
				hrp.AssemblyLinearVelocity = Vector3.zero
				SetNetworkOwner:FireServer(Drawer.Part,Drawer.Part.CFrame)
				wait(0.05)
				hrp.CFrame = OriginalPosition
				hrp.AssemblyLinearVelocity = Vector3.zero
				if Drawer.PrimaryPart:FindFirstChild("BodyVelocity") == nil and DrawerCannonAutoShoot_Toggle == true then
					Drawer.Name = "Trashed"
					if Drawer.PrimaryPart:FindFirstChild("BodyPosition") then
						Drawer.PrimaryPart:FindFirstChild("BodyPosition"):Destroy()
					end
					local velocityObj = Instance.new("BodyVelocity", Drawer.PrimaryPart)
					velocityObj.MaxForce = Vector3.new(math.huge, math.huge, math.huge)
					velocityObj.Velocity = (workspace.CurrentCamera.CFrame.lookVector * 1000)-Vector3.new(0,500,0)
					task.spawn(function()
						wait(6)
						DestroyToy:FireServer(Drawer)
					end)
				end
			end
		end
		for _, Drawer in ipairs(SpawnedInToys:GetChildren()) do
			if Drawer:IsA("Model") and Drawer.Name == "DrawerLightBrown"  then
				if Drawer.PrimaryPart:FindFirstChild("BodyPosition") then
					local BodyPosition = Drawer.PrimaryPart:FindFirstChild("BodyPosition")
					BodyPosition.Position = hrp.Position + Vector3.new(0,50,0)
					BodyPosition.MaxForce = Vector3.new(math.huge,math.huge,math.huge)
					BodyPosition.Parent = Drawer.PrimaryPart
					BodyPosition.P = 99999
					BodyPosition.D = 500
				else
					local BodyPosition = Instance.new("BodyPosition")
					BodyPosition.Position = hrp.Position + Vector3.new(0,50,0)
					BodyPosition.MaxForce = Vector3.new(math.huge,math.huge,math.huge)
					BodyPosition.Parent = Drawer.PrimaryPart
					BodyPosition.P = 99999
					BodyPosition.D = 500
				end
			end
		end
		wait()
	end
	KeyBindShoot:Disconnect()
	for _, Drawer in ipairs(SpawnedInToys:GetChildren()) do
		if Drawer:IsA("Model") and Drawer.Name == "DrawerLightBrown" then
			if Drawer.PrimaryPart:FindFirstChild("BodyPosition") then
				Drawer.PrimaryPart:FindFirstChild("BodyPosition"):Destroy()
			end
		end
	end
end
function CrazyLine()
	while CrazyLine_Toggle == true do
		for _, Player in ipairs(game.Players:GetPlayers()) do
			if Player.Character and Player ~= plr then
				game.ReplicatedStorage.GrabEvents.CreateGrabLine:FireServer(Player.Character.Head,CFrame.new(0,0,0))
				wait()
			end
		end
		wait()
	end
end
