	Name = "Credits",
	Icon = "rbxassetid://116947503696913",
	PremiumOnly = false
})


PlayerTab:AddSection({Name = "Player Stats"})

PlayerTab:AddToggle({
	Name = "Walk Speed",
	Default = false,
	Callback = function(Value)
		WalkSpeed_Toggle = Value
	end  
})
PlayerTab:AddSlider({
	Name = "Speed",
	Min = 16,
	Max = 200,
	Default = 16,
	Color = Color3.fromRGB(255, 255, 255),
	Increment = 1,
	ValueName = "Speed",
	Callback = function(Value)
		WalkSpeed_Value = Value
	end    
})
PlayerTab:AddToggle({
	Name = "JumpPower",
	Default = false,
	Callback = function(Value)
		JumpPower_Toggle = Value
		if Value == false then
			plr.Character.Humanoid.JumpPower = 30
		end
	end  
})
PlayerTab:AddSlider({
	Name = "Power",
	Min = 30,
	Max = 200,
	Default = 16,
	Color = Color3.fromRGB(255, 255, 255),
	Increment = 1,
	ValueName = "Power",
	Callback = function(Value)
		plr.Character.Humanoid.JumpPower = Value
	end    
})

PlayerTab:AddSection({Name = "Player Functions"})

PlayerTab:AddToggle({
	Name = "Locked Camera",
	Default = true,
	Callback = function(Value)
		if Value == true then
			game.Players.LocalPlayer.CameraMode = Enum.CameraMode.LockFirstPerson
			game.Players.LocalPlayer.CameraMaxZoomDistance = 0.5
		else
			game.Players.LocalPlayer.CameraMode = Enum.CameraMode.Classic
			game.Players.LocalPlayer.CameraMaxZoomDistance = 100000
		end
	end  
})
PlayerTab:AddToggle({
	Name = "Infinite Jump",
	Default = false,
	Callback = function(Value)
		InfiniteJump_Toggle = Value
	end  
})
PlayerTab:AddToggle({
	Name = "Invisible",
	Default = false,
	Callback = function(Value)
		if Value == true then
			Invisible_Function = game:GetService("RunService").RenderStepped:Connect(function()
				game.ReplicatedStorage.CharacterEvents.Look:FireServer(CFrame.new(Vector3.new(0,0,0)), CFrame.new(Vector3.new(0,-20,0)),CFrame.new(Vector3.new(0,0,0)), "high")
				plr:WaitForChild("PlayerScripts"):WaitForChild("CharacterAndBeamMove").Enabled = false
			end)
			for _, Part in ipairs(plr.Character:GetChildren()) do
				if Part:IsA("BasePart") and Part.Name ~= "HumanoidRootPart" or Part.Name ~= "CamPart" then
					Part.Transparency = 1
				end
			end
		else
			Invisible_Function:Disconnect()
			plr:WaitForChild("PlayerScripts"):WaitForChild("CharacterAndBeamMove").Enabled = true
			for _, Part in ipairs(plr.Character:GetChildren()) do
				if Part:IsA("BasePart") and Part.Name ~= "HumanoidRootPart" or Part.Name ~= "CamPart" then
					Part.Transparency = 0
				end
			end
		end
	end  
})
PlayerTab:AddToggle({
	Name = "Headless",
	Default = false,
	Callback = function(Value)
		if Value == true then
			game.ReplicatedStorage.CharacterEvents.Look:FireServer(CFrame.new(Vector3.new(0,-100,0)), CFrame.new(Vector3.new(0,0,0))*CFrame.Angles(math.rad(90),math.rad(180),math.rad(0)),CFrame.new(Vector3.new(1,0.5,0))*CFrame.Angles(math.rad(0),math.rad(90),math.rad(0)), "high")
			plr:WaitForChild("PlayerScripts"):WaitForChild("CharacterAndBeamMove").Enabled = false
			wait(0.1)
			game.ReplicatedStorage.CharacterEvents.Look:FireServer(CFrame.new(Vector3.new(0,-100,0)), CFrame.new(Vector3.new(0,0,0))*CFrame.Angles(math.rad(90),math.rad(180),math.rad(0)),CFrame.new(Vector3.new(1,0.5,0))*CFrame.Angles(math.rad(0),math.rad(90),math.rad(0)), "high")
		else
			plr:WaitForChild("PlayerScripts"):WaitForChild("CharacterAndBeamMove").Enabled = true
		end
	end  
})
PlayerTab:AddToggle({
	Name = "Noclip",
	Default = false,
	Callback = function(Value)
		if Value == true then
			Noclip_Function = game:GetService("RunService").RenderStepped:Connect(function()
				plr.Character:WaitForChild("Torso").CanCollide = false
				plr.Character:WaitForChild("Head").CanCollide = false
				plr.Character:WaitForChild("Torso").Changed:Connect(function()
					if Noclip_Function ~= nil then
						plr.Character:WaitForChild("Torso").CanCollide = false
						plr.Character:WaitForChild("Head").CanCollide = false
					end
				end)

			end)
		else
			Noclip_Function:Disconnect()
			Noclip_Function = nil
			plr.Character:WaitForChild("Torso").CanCollide = true
			plr.Character:WaitForChild("Head").CanCollide = true
		end
	end  
})

ImmunityTab:AddSection({Name = "Invincibility"})

ImmunityTab:AddToggle({
	Name = "Anti-Grab",
	Default = false,
	Callback = function(Value)
		if Value == true then
			AntiGrab_Function = game:GetService("RunService").RenderStepped:Connect(function()
				local chr = plr.Character
				local HRP = chr:WaitForChild("HumanoidRootPart")
				if plr:WaitForChild("IsHeld").Value == true then
					if HRP ~= nil then
						HRP.Anchored = true
						while plr.IsHeld.Value == true do game.ReplicatedStorage.CharacterEvents.Struggle:FireServer(plr);wait(0.001) end
						HRP.Anchored = false
					end
				end
			end)
		else
			AntiGrab_Function:Disconnect()
		end
	end    
})
ImmunityTab:AddToggle({
	Name = "Anti-Fire",
	Default = false,
	Callback = function(Value)
		if Value == true then
			AntiFire_Function = game:GetService("RunService").RenderStepped:Connect(function()
				local chr = plr.Character
				local HRP = chr:WaitForChild("HumanoidRootPart")
				if chr:WaitForChild("Humanoid"):WaitForChild("FireDebounce").Value == true then
					local Part = workspace.Map.Hole.PoisonBigHole.ExtinguishPart
					Part.Size = Vector3.new(5,5,5)
					Part.CastShadow = false
					Part.Transparency = 1
					if Part:FindFirstChild("Tex") then
						Part:FindFirstChild("Tex"):Destroy()
					end
					if HRP ~= nil then
						Part.Position = HRP.Position+Vector3.new(0,20,0)
						wait(0.05)
						Part.Position = HRP.Position
					end
				end
			end)
		else
			AntiFire_Function:Disconnect()
		end
	end    
})
ImmunityTab:AddToggle({
	Name = "Anti-Explosion V1",
	Default = false,
	Callback = function(Value)
		if Value == true then
			plr:WaitForChild("PlayerScripts"):WaitForChild("ClientExoplosionHandler").Enabled = false
		else
			plr:WaitForChild("PlayerScripts"):WaitForChild("ClientExoplosionHandler").Enabled = true
		end
	end    
})
ImmunityTab:AddToggle({
	Name = "Anti-Explosion V2",
	Default = false,
	Callback = function(Value)
		if Value == true then
			AntiExplosionV2_Function = workspace.ChildAdded:Connect(function(model)
				local chr = plr.Character
				local hrp = chr:WaitForChild("HumanoidRootPart")
				local hum = chr:WaitForChild("Humanoid")
				if model.Name == "Part" and chr ~= nil then
					local mag = (model.Position - hrp.Position).Magnitude
					if mag <= 20 then
						hrp.Anchored = true
						wait(0.01)
						while chr["Right Arm"].RagdollLimbPart.CanCollide == true do wait(0.001) end
						hrp.Anchored = false
					end
				end
			end)
		else
			AntiExplosionV2_Function:Disconnect()
		end
	end    
})
local AntiSticky = ImmunityTab:AddToggle({
	Name = "Anti-Sticky",
	Default = false,
	Callback = function(Value)
		if Value == true then
			local chr = plr.Character
			local HRP = chr:WaitForChild("HumanoidRootPart")
			AntiSticky_Function = game:GetService("RunService").RenderStepped:Connect(function()
				local chr = plr.Character
				local HRP = chr:WaitForChild("HumanoidRootPart")
				if SpawnedInToys:FindFirstChild("SprayCanWD") == nil then
					SpawnToyRemoteFunction:InvokeServer("SprayCanWD",CFrame.new(HRP.Position+Vector3.new(0,10,0)),Vector3.new(0,0,0))
					local SprayCanWD = SpawnedInToys:WaitForChild("SprayCanWD")
					local BP = Instance.new("BodyPosition")
					local Original = HRP.CFrame
					HRP.CFrame = SprayCanWD.PrimaryPart.CFrame
					wait(0.05)
					SetNetworkOwner:FireServer(SprayCanWD:FindFirstChild("Straw"),SprayCanWD:FindFirstChild("Straw").CFrame)
					wait(0.05)
					HRP.CFrame = SprayCanWD.PrimaryPart.CFrame
					wait(0.05)
					SetNetworkOwner:FireServer(SprayCanWD:FindFirstChild("Straw"),SprayCanWD:FindFirstChild("Straw").CFrame)
					BP.Position = Vector3.new(0,500,0)
					BP.MaxForce = Vector3.new(math.huge,math.huge,math.huge)
					BP.Parent = SprayCanWD.PrimaryPart
					wait(0.05)
					HRP.AssemblyLinearVelocity = Vector3.zero
					HRP.CFrame = Original
					return
				else
					local SprayCanWD = SpawnedInToys:FindFirstChild("SprayCanWD")

					SprayCanWD:FindFirstChild("StickyRemoverPart").Size = Vector3.new(5,5,5)
					SprayCanWD:FindFirstChild("StickyRemoverPart").Position = HRP.Position
				end
			end)
		else
			AntiSticky_Function:Disconnect()
			AntiSticky_Function = nil
		end
	end    
})
ImmunityTab:AddToggle({
	Name = "Anti-Kick",
	Default = false,
	Callback = function(Value)
		if Value == true then
			local chr = plr.Character
			local HRP = chr:WaitForChild("HumanoidRootPart")
			AntiKick_Function = game:GetService("RunService").RenderStepped:Connect(function()
				local chr = plr.Character
				local HRP = chr:WaitForChild("HumanoidRootPart")
				if SpawnedInToys:FindFirstChild("NinjaShuriken") == nil then
					SpawnToyRemoteFunction:InvokeServer("NinjaShuriken",CFrame.new(Vector3.new(0,0,0)),Vector3.new(0,0,0))

					local NinjaShuriken = SpawnedInToys:WaitForChild("NinjaShuriken")
					game.ReplicatedStorage.PlayerEvents.StickyPartEvent:FireServer(NinjaShuriken.StickyPart,plr.Character.Torso,CFrame.new(Vector3.new(0,-10,0)))
					return
				else
					local NinjaShuriken = SpawnedInToys:WaitForChild("NinjaShuriken")
					if NinjaShuriken.StickyPart.StickyWeld.Part1 == nil then
						game.ReplicatedStorage.PlayerEvents.StickyPartEvent:FireServer(NinjaShuriken.StickyPart,plr.Character.Torso,CFrame.new(Vector3.new(0,-10,0)))
					end
				end
			end)
		else
			AntiKick_Function:Disconnect()
		end
	end    
})
ImmunityTab:AddToggle({
	Name = "Anti-Lag",
	Default = false,
	Callback = function(Value)
		if Value == true then
			plr:WaitForChild("PlayerScripts"):WaitForChild("CharacterAndBeamMove").Enabled = false
		else
			plr:WaitForChild("PlayerScripts"):WaitForChild("CharacterAndBeamMove").Enabled = true
		end
	end    
})
ImmunityTab:AddToggle({
	Name = "Anti-Void",
	Default = false,
	Callback = function(Value)
		if Value == true then
			workspace.FallenPartsDestroyHeight = -math.huge
		else
			workspace.FallenPartsDestroyHeight = -100
		end
	end    
})

ImmunityTab:AddSection({Name = "Invincibility Others"})

ImmunityTab:AddParagraph("This allows you to give other players abilities","Note: You must be in game for it to work")

local ImmunityTarget_DropDown = ImmunityTab:AddDropdown({
	Name = "Target",
	Default = "...",
	Options = GetPlayerList(false,true),
	Callback = function(Value)
		if GetPlayerFromDisplayName(Value) == nil then
			return
		end
		ImmunityTarget_Value = GetPlayerFromDisplayName(Value).Name
	end    
})
ImmunityTab:AddToggle({
	Name = "Anti-Grab",
	Default = false,
	Callback = function(Value)
		if Value == true then
			OtherAntiGrab_Function = game:GetService("RunService").RenderStepped:Connect(function()
				if game.Players:FindFirstChild(ImmunityTarget_Value) and game.Players:FindFirstChild(ImmunityTarget_Value).Character then
					local Character = game.Players:FindFirstChild(ImmunityTarget_Value).Character
					local HRP = plr.Character.HumanoidRootPart
					for _, PartOwner in ipairs(Character:GetDescendants()) do
						if PartOwner:IsA("StringValue") and PartOwner.Name == "PartOwner" and PartOwner.Value ~= plr.Name then
							local Original = HRP.CFrame
							local Original2 = Character.Torso.CFrame
							HRP.AssemblyLinearVelocity = Vector3.zero
							HRP.CFrame = CFrame.new(Character.Torso.Position+Vector3.new(0,10,0))
							wait(0.05)
							HRP.AssemblyLinearVelocity = Vector3.zero
							HRP.CFrame = CFrame.new(Character.Torso.Position+Vector3.new(0,10,0))
							wait(0.05)
							SetNetworkOwner:FireServer(Character.Torso,Character.Torso.CFrame)
							wait(0.05)
							HRP.AssemblyLinearVelocity = Vector3.zero
							HRP.CFrame = CFrame.new(Character.Torso.Position+Vector3.new(0,10,0))
							wait(0.05)
							SetNetworkOwner:FireServer(Character.Torso,Character.Torso.CFrame)
							wait(0.05)
							HRP.AssemblyLinearVelocity = Vector3.zero
							HRP.CFrame = CFrame.new(Character.Torso.Position+Vector3.new(0,10,0))
							wait(0.05)
							SetNetworkOwner:FireServer(Character.Torso,Character.Torso.CFrame)
							Character.Torso.CFrame = Original2
							Character.Torso.AssemblyLinearVelocity = Vector3.zero
							DestroyGrabline:FireServer(Character.Torso)
							HRP.AssemblyLinearVelocity = Vector3.zero
							HRP.CFrame = Original
						end
					end
				end
			end)
		else
			OtherAntiGrab_Function:Disconnect()
		end
	end    
})

