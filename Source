MiskTab:AddSection({Name = "Annoy Players"})
MiskTab:AddToggle({
	Name = "Fire All",
	Default = false,
	Callback = function(Value)
		if Value == true then
			FireAll_Function = game:GetService("RunService").RenderStepped:Connect(function()
				local chr = plr.Character
				local HRP = chr:WaitForChild("HumanoidRootPart")
				if SpawnedInToys:FindFirstChild("Campfire") then
					local Campfire = SpawnedInToys:FindFirstChild("Campfire")
					if Campfire.PrimaryPart:FindFirstChild("BodyPosition") == nil then
						local BP = Instance.new("BodyPosition")
						BP.MaxForce = Vector3.new(math.huge,math.huge,math.huge)
						BP.Position = Vector3.new(0,250,0)
						BP.Parent = Campfire.PrimaryPart
						BP.P = 99999
					end
					if Campfire:FindFirstChild("SoundPart"):FindFirstChild("PartOwner") == nil then
						local Original = HRP.CFrame
						SetNetworkOwner:FireServer(Campfire:FindFirstChild("SoundPart"),Campfire:FindFirstChild("SoundPart").CFrame)
						HRP.CFrame = Campfire:FindFirstChild("SoundPart").CFrame
						wait(0.05)
						HRP.CFrame = Original
					end
					Campfire:FindFirstChild("FirePlayerPart").Size = Vector3.new(99999,99999,99999)
				else
					SpawnToyRemoteFunction:InvokeServer("Campfire",HRP.CFrame,Vector3.new(0,0,0))
				end
			end)
		else
			if SpawnedInToys:FindFirstChild("Campfire") then
				local Campfire = SpawnedInToys:FindFirstChild("Campfire")
				Campfire:FindFirstChild("FirePlayerPart").Size = Vector3.new(2, 2, 2)
				Campfire:FindFirstChild("FirePlayerPart").Anchored = false
			end
			FireAll_Function:Disconnect()
		end
	end    
})
MiskTab:AddToggle({
	Name = "Kill All",
	Default = false,
	Callback = function(Value)
		KillAll_Toggle = Value
		wait(0.05)
		if Value == true then
			KillAll()
		end
	end    
})
MiskTab:AddToggle({
	Name = "Radiation All",
